<%- include("./partials/header") %>
<%- include("./partials/navbar") %>
<div class="bg-success p-2 text-dark bg-opacity-10"></div>
<div class="container mt-5 py-5">
    <h3>Here, you can find 'Interview Experiences'</h3>
    <% blogs.reverse() %> 
    <% blogs.forEach(blog => { %>
        <div class="card border-primary mt-3 ">
            <div class="card-body">
                <p class="text-muted">published at: <%= blog.postedAt %> </p>
                <h4 class="card-title"><%= blog.title %> </h4>
                <p class="card-text"><%= blog.content.slice(0, 99) %> .... <a href="/blog/<%= blog._id %>">Read More</a></p>
            <a href=" /blog/<%= blog._id %>" class="btn btn-success">Read More</a>
            <a href=" /edit/<%= blog._id %>" class="btn btn-danger">Edit</a>
            <a href=" /delete/<%= blog._id %>" class="btn btn-danger">delete</a>
            </div>
        </div>
        <%}) %>

</div>
<%- include("./partials/footer") %>